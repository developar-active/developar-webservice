{
   "version": 2,
   "builds": [
      {
         "src": "index.js",
         "use": "@now/node-server"
      },
      { "src": "public/", "use": "@vercel/static-build" }
   ],
   "routes": [
      {
         "src": "/.*",
         "dest": "index.js",
         "methods": [
            "GET",
            "POST"
         ]
      }
   ]
}