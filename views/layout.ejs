   <meta charset="UTF-8" />
   <meta name="keywords" content="<%- keywords %>" />

   <% if (hasCustomDescription) { %>
      <meta name="description" content="<%- PAGE_DESCRIPTION %>" />
   <% } else { %>
      <meta name="description" content="Developar is a company which focusing on web technologies for producing high quality apps and building such tools for helping developers, responsive website, developing cross platform mobile apps by using modern frameworks and provide intuitive marketing facility to grow your business or individual." />
   <% } %>

