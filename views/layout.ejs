<% 
   /** 
    * For entire site
    * @type {Array<string>}
    */
   const GLOBAL_KEYWORDS = ['Developar', 'Developar Company'];

   /**
    * Check string type existance
    * @param {string} property - Property name
    * @returns {boolean}
    */
   function existsStringValue (property) {
      return (typeof locals[property] === 'string' && locals[property].length > 0);
   }

   /**
    * Check array type existance
    * @param {string} property - Property name
    * @returns {boolean}
    */
   function existsArrayValue (property) {
      return (typeof locals[property] === 'object' && Array.isArray(locals[property]));
   }

   let keywords = '';
   let hasCustomDescription = existsStringValue('PAGE_DESCRIPTION');

   if (existsArrayValue('PAGE_KEYWORDS')) {
      const customKeywords = GLOBAL_KEYWORDS.concat(PAGE_KEYWORDS || []);

      keywords = customKeywords.join(', ');
   }
%>

