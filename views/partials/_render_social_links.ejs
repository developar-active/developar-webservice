<% if (links && links.length > 0) { %>
<div class="contacts">
   <ul>
      <% for (const link of links) { %>
      <li>
         <a href="<%= link.url %>" title="<%= link.name %>" target="_blank">
            <i data-feather="<%= link.icon %>"></i>
         </a>
      </li>
      <% } %>
   </ul>
</div>
<% } %>